@import '/assets/css/sprites.wxss';
page{
  background-color: #f5f5f5;
}
.set-list{
  margin: 24rpx 30rpx;
  padding: 0 30rpx;
  background-color: #fff;
  border-radius: 22rpx;
}
.set-list .item{
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 29rpx 0;
}
.default-til{
  display: flex;
  align-items: center;
}
.set-list label{
  width: 46rpx;
  height: 46rpx;
  margin-right: 16rpx;
}
.set-list text{
  color: #999;
  font-size: 32rpx;
}
.set-list .default-til text{
  color: #4e4e4e;
  font-size: 28rpx;
}
