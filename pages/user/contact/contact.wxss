page{
  background-color: #f5f5f5;
}
.contcat{
  padding-top: 124rpx;
  margin: 0 80rpx;
}
.wxCode{
  height: 606rpx;
  background-color: #fff;
  padding-top: 86rpx;
  border-radius: 22rpx;
  position: relative;
}
.logo{
  width: 160rpx;
  height: 160rpx;
  border: 14rpx solid #f5f5f5;
  position: absolute;
  top: -116rpx;
  left: 50%;
  transform: translateX(-50%);
  border-radius: 50%;
  overflow: hidden;
}
.logo text{
  display: block;
  height: 100%;
  color: #fff;
  font-size: 60rpx;
  line-height: 160rpx;
  text-align: center;
  background-color: #ff4964;
}
.code{
  width: 440rpx;
  height: 440rpx;
  margin: 0 auto 52rpx auto;
  background-color: #eee;
  overflow: hidden;
}
.wxCode button{
  width: 344rpx;
  height: 68rpx;
  line-height: 68rpx;
  font-size: 34rpx;
  color: #fff;
  background-color: #ff4964;
  margin: 0 auto;
  border-radius: 68rpx;
}
.more{
  font-size: 42rpx;
  color: #222;
  text-align: center;
  margin: 20rpx 0;
}
.contcat-list .item{
  display: flex;
  justify-content: space-between;
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 2rpx solid #ddd;
}
.contcat-list .item text{
  font-size: 26rpx;
  color: #222;
}
.contcat-list .item view{
  width: 156rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #fff;
  font-size: 34rpx;
  background-color: #333;
}
.copy-msg{
  color: #222;
  text-align: center;
  margin-top: 26rpx;
}